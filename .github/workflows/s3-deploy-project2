name: Deploy HotelReservation React

on:
  push:
    branches:[staging]

jobs:
  build:
    runs-on: ubuntu-latest
    steps:
      - name: Checkout code
        uses: actions/checkout@v3
      - name: Install Packages
        run: npm install
      - name: Build React App
        run: npm run build
        # Deploy to S3
       
       
       

    
